                    android:layout_centerInParent="true"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_toLeftOf="@id/dotiv3"
                    android:text="Dr. Vishal"
                    android:paddingLeft="15dp"/>
            </RelativeLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:id="@+id/doc4cv"
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            card_view:cardBackgroundColor="#FDFDFD"
            card_view:cardCornerRadius="5dp"
            card_view:cardElevation="2dp"
            android:layout_below="@id/doc3cv"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            card_view:cardUseCompatPadding="true"
            xmlns:tools="http://schemas.android.com/tools">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/dotiv4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="4dp"
                    android:src="@drawable/green"/>
                <TextView
                    android:id="@+id/doc4tv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_toLeftOf="@id/dotiv4"
                    android:text="Dr. Umesh"
                    android:paddingLeft="15dp"/>
            </RelativeLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:id="@+id/doc5cv"
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            card_view:cardBackgroundColor="#FDFDFD"
            card_view:cardCornerRadius="5dp"
            card_view:cardElevation="2dp"
            android:layout_below="@id/doc4cv"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            card_view:cardUseCompatPadding="true"
            xmlns:tools="http://schemas.android.com/tools">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/dotiv5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="4dp"
                    android:src="@drawable/green"/>
                <TextView
                    android:id="@+id/doc5tv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_toLeftOf="@id/dotiv5"
                    android:text=" Dr. Sunidhi"
                    android:paddingLeft="15dp"/>
            </RelativeLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:id="@+id/doc6cv"
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            card_view:cardBackgroundColor="#FDFDFD"
            card_view:cardCornerRadius="5dp"
            card_view:cardElevation="2dp"
            android:layout_below="@id/doc5cv"
